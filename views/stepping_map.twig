<html>

<head>
	<title>踩踏水金九黃金礦業</title>
	<meta charset="utf-8" />
	<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
	<link rel="stylesheet" href="assets/css/main.css" />
	<link rel="stylesheet" href="assets/css/swiper.min.css" />
	<link rel="stylesheet" href="assets/css/magnific-popup.css" />

	<link rel="stylesheet" href="css/all.css" />
	<link rel="stylesheet" href="css/module.css" />

<!-- 	<link href="http://vjs.zencdn.net/c/video-js.css" rel="stylesheet" />
	<script src="http://vjs.zencdn.net/c/video.js"></script>
 -->


	<!--燈相-->
	<!-- Scripts -->
	<script src="assets/js/jquery.min.js"></script>
	<script src="assets/js/jquery.scrollex.min.js"></script>
	<script src="assets/js/jquery.scrolly.min.js"></script>
	<script src="assets/js/browser.min.js"></script>
	<script src="assets/js/breakpoints.min.js"></script>
	<script src="assets/js/util.js"></script>
	<script src="js/map_ui.js"></script>

	<script type="text/javascript" src="js/jquery.magnific-popup.min.js"></script>
	<style>
		#navPanelToggle{display: none}
	</style>

</head>

<body class="is-preload">
		
	
	<!-- Wrapper -->
	<div id="wrapper">
		
		
		{% include '_template/menu.twig' %}

		<br>
	
			
		<!-- Main -->
		<div id="main">
			<!-- Post -->
			<section class="map_block">


				<h1>踩踏水金九黃金礦業 </h1>
				

				<div class="map_section">
					
					<div class="this_map">

						{% for item in position.map_tag %}
							<div class="location_position" style="{{item.style }}">
								<div class="location">
									<i class="fas fa-map-marker-alt"></i>
								</div>

								<div class="location_detail">
									<div class="detail_row">
										<div class="des">
											<h6>{{item.name }}</h6>
											<div class="gotomap">{{item.location}}</div>
											<p>{{item.description}}</p>
										</div>

										<img src="{{item.img}}" alt="">
									</div>

									<div class="detail_row">
										<div class="video-link">
											<div class="">{{item.link_a}}</div>
											<div class="">{{item.link_b}}</div>
											<div class="">{{item.link_c}}</div>
											<div class="">{{item.link_d}}</div>
											<div class="">{{item.link_e}}</div>
											<div class="">{{item.link_f}}</div>
										</div>
									</div>
								</div>
							</div>

						{% endfor %} 

						<div class="bg_map"><img src="img/map.jpg" alt=""></div>
					</div>

				</div>




			</section>
		</div>
		
		<br>
		<br>
		<br>
		<br>
		<br>

	
		<!-- Copyright -->
		<div id="copyright">
			<ul>
				<li>文化部&copy; Copyright 2018</li>

			</ul>
			<ul style="color: #777">
				<li>&copy; Copyright 2018</li>
				<li>Design: <a href="https://html5up.net">HTML5 UP</a></li>
			</ul>
		</div>

		<div class="bg fixed" style="transform: none;"></div>

		<button class="goto-top">
			回頂端
		</button>
	</div>


	<script src="assets/js/main.js"></script>
</body>

</html>